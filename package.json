	"scripts": {
		"start": "nx serve",
		"build": "nx build",
		"jest": "env TZ='Europe/Berlin' jest --runInBand",
		"test": "env TZ='Europe/Berlin' nx run-many --target=test --all --runInBand",
		"test:ci": "env TZ='Europe/Berlin' nx run-many --target=test --all --cache=false --skip-nx-cache --runInBand",
		"cypress": "cypress open --project=apps/site-e2e",
		"storybook": "env TAILWIND_MODE=watch nx run docs-storybook:storybook --watch",
		"prettier:check": "prettier --check **/*",
		"prettier:write": "prettier --write **/*",
		"prisma": "prisma",
		"algolia:sync": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' --project libs/feature/search/tsconfig.json --require tsconfig-paths/register libs/feature/search/src/lib/algolia-sync.ts",
		"start:prod": "cd dist/apps/site && npm run start"
	},
	"private": true,
	"prisma": {
		"schema": "libs/data-access/database/prisma/schema.prisma",
		"seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} --project libs/data-access/database/tsconfig.json --require tsconfig-paths/register libs/data-access/database/src/lib/seed.ts"
