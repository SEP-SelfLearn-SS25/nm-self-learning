{
	"root": "docs/storybook",
	"sourceRoot": "docs/storybook/src",
	"projectType": "library",
	"tags": [],
	"targets": {
		"lint": {
			"executor": "@nrwl/linter:eslint",
			"outputs": ["{options.outputFile}"],
			"options": {
				"lintFilePatterns": ["docs/storybook/**/*.{ts,tsx,js,jsx}"]
			}
		},
		"test": {
			"executor": "@nrwl/jest:jest",
			"outputs": ["coverage/docs/storybook"],
			"options": {
				"jestConfig": "docs/storybook/jest.config.js",
				"passWithNoTests": true
			}
		},
		"storybook": {
			"executor": "@nrwl/storybook:storybook",
			"options": {
				"uiFramework": "@storybook/react",
				"port": 4400,
				"config": {
					"configFolder": "docs/storybook/.storybook"
				}
			},
			"configurations": {
				"ci": {
					"quiet": true
				}
			}
		},
		"build-storybook": {
			"executor": "@nrwl/storybook:build",
			"outputs": ["{options.outputPath}"],
			"options": {
				"uiFramework": "@storybook/react",
				"outputPath": "dist/storybook/docs-storybook",
				"config": {
					"configFolder": "docs/storybook/.storybook"
				}
			},
			"configurations": {
				"ci": {
					"quiet": true
				}
			}
		}
	}
}
